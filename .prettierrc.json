{
  "$schema": "https://json.schemastore.org/prettierrc",
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 100,
  "trailingComma": "es5",
  "tailwindFunctions": ["clsx", "cva"],
  "importOrder": ["^vue$", "<THIRD_PARTY_MODULES>", "^@/(.*)$", "^[.]"]
}
